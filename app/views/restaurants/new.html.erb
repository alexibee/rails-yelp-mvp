<div class="container">
<%= simple_form_for(@restaurant) do |f| %>
      <%= f.input :name %>
      <%= f.input :address %>
      <%= f.input :phone_number %>
      <%= f.input :category, :collection => Restaurant::CATEGORIES %>
      <%= f.submit "Add restaurant", class:"btn btn-primary" %>
  <% end %>
</div>
<%= link_to "Back to all restaurants", restaurants_path %>
