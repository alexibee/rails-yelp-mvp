<div class="container">
<%= simple_form_for(@review) do |f| %>
      <%= f.input :content %>
      <%= f.input :rating, collection: (0..5) %>
      <%= f.submit "Submit", class:"btn btn-primary" %>
  <% end %>
</div>
<%= link_to "Back to the restaurant page", restaurant_path(@review[:restaurant_id]) %>
